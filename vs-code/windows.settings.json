{
  "debug.javascript.codelens.npmScripts": "never",
  "editor.fontFamily": "'Cascadia Code', Pretendard",
  "editor.fontSize": 15,
  "editor.tabSize": 2,
  "files.eol": "\n",
  "github.copilot.enable": {
    "*": true,
    "markdown": true
  },
  "remote.SSH.remotePlatform": {
    "retn0.iptime.org": "linux"
  },
  "terminal.integrated.fontFamily": "'Cascadia Mono', Pretendard",
  "terminal.integrated.fontSize": 15,
  "workbench.iconTheme": "material-icon-theme",
  "[c]": {
    "editor.tabSize": 4
  },
  "[cpp]": {
    "editor.tabSize": 4
  },
  "[dockerfile]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[javascriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[lua]": {
    "editor.tabSize": 4
  },
  "[markdown]": {
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  }
}
